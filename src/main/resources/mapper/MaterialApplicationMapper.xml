<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="icu.shishc.applet.mapper.CommunityMapper">
    <resultMap id="BaseResultMap" type="icu.shishc.applet.entity.MaterialApplication">
        <id column="application_id" property="applicationId"/>
        <id column="applicant_id" property="applicantId"/>
        <id column="applicant_name" property="applicantName"/>
        <id column="applicant_reason" property="applicantReason"/>
        <id column="material_name" property="materialName"/>
        <id column="material_num" property="materialNum"/>
        <id column="is_urgency" property="isUrgency"/>
        <id column="location" property="location"/>
        <id column="approver_id" property="approverId"/>
        <id column="result" property="result"/>
        <id column="create_time" property="createTime"/>
        <id column="approval_time" property="approvalTime"/>
        <id column="refuse-reason" property="refuseReason"/>
    </resultMap>

    <insert id="addMaterialApplication" parameterType="icu.shishc.applet.entity.MaterialApplication" useGeneratedKeys="true" keyColumn="application_id" keyProperty="applicationId">
        insert into material_application(applicant_id,
                                         applicant_name,
                                         applicant_reason,
                                         material_name,
                                         material_num,
                                         is_urgency,
                                         location,
                                         create_time,
                                         approver_id)
                 values (#{applicantId}, #{applicantName}, #{applicantReason}, #{materialName}, #{materialNum},
                         #{isUrgency}, #{location}, #{createTime}, #{approverId})
    </insert>

</mapper>