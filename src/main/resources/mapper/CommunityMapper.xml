<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="icu.shishc.applet.mapper.CommunityMapper">
    <resultMap id="BaseResultMap" type="icu.shishc.applet.entity.Community">
        <id column="community_id" property="communityId"/>
        <id column="community_name" property="communityName"/>
        <id column="address" property="address"/>
    </resultMap>

    <insert id="addCommunity" parameterType="icu.shishc.applet.entity.Community" useGeneratedKeys="true" keyColumn="community_id" keyProperty="communityId">
        insert into community(community_name, address) values (#{communityName}, #{address})
    </insert>

</mapper>